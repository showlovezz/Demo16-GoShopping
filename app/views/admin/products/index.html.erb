<section class="mains_products_index">
	<div class="container-fluid">
		<div class="row">
			<!-- left sidebar -->
			<div class="sidebar col-12 col-sm-4 col-md-4 col-lg-2 mt-4">
				<%= render "admin/shared/sidebar" %>
			</div>

			<!-- right content -->
			<div class="sidecontent col-12 col-sm-8 col-md-8 col-lg-10 mt-4">
				<!-- breadcrumb -->
				<%= render "breadcrumb" %>

				<div class="widget">
					<!-- tables header -->
					<%= render "table_header" %>
					<div class="widget-body">
						<!-- nav -->
						<%= render "nav" %>
						<div class="tab-content" id="myTabContent">
						  <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
						  	<!-- products -->
						  	<%= render "table" %>
						  </div>
						  <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
						  	<!-- new form -->
								<%= render "form" %>
						  </div>
						  <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">Text....</div>
						</div>
					</div>
				</div>
			</div>
		
		</div>
	</div>
</section>